{
    "name": "orchestra/platform",
    "description": "Orchestra Platform",
    "homepage": "http://orchestraplatform.com/docs/latest/",
    "keywords": [
        "framework",
        "laravel",
        "orchestra-platform",
        "orchestral"
    ],
    "license": "MIT",
    "type": "project",
    "require": {
        "php": "^8.0.2",
        "fideloper/proxy": "^4.4",
        "fruitcake/laravel-cors": "^3.0",
        "guzzlehttp/guzzle": "^7.2",
        "laravel/tinker": "^2.7",
        "orchestra/avatar": "7.x-dev",
        "orchestra/control": "7.x-dev",
        "orchestra/foundation": "7.x-dev",
        "orchestra/installer": "7.x-dev"
    },
    "require-dev": {
        "facade/ignition": "^2.0",
        "mockery/mockery": "^1.4.4",
        "nunomaduro/collision": "^6.1",
        "orchestra/testing": "7.x-dev",
        "phpunit/phpunit": "^9.5.10"
    },
    "autoload": {
        "classmap": [
            "database/factories",
            "database/seeds"
        ],
        "psr-4": {
            "App\\": "app/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        },
        "config-cache": []
    },
    "scripts": {
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ],
        "post-autoload-dump": [
            "Orchestra\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi",
            "@php artisan orchestra:assemble"
        ]
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    },
    "prefer-stable": true,
    "minimum-stability": "dev",
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/katsana-external/foundation"
        },
        {
            "type": "vcs",
            "url": "https://github.com/katsana-external/avatar"
        },
        {
            "type": "vcs",
            "url": "https://github.com/katsana-external/installer"
        },
        {
            "type": "vcs",
            "url": "https://github.com/katsana-external/control"
        },
        {
            "type": "vcs",
            "url": "https://github.com/katsana-external/testing"
        }
    ]
}
